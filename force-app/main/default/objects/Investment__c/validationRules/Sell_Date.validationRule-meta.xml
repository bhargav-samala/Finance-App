<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Sell_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISNULL(Sell_Date__c ), NOT( ISNULL( Sell_Price__c ) ))</errorConditionFormula>
    <errorDisplayField>Sell_Date__c</errorDisplayField>
    <errorMessage>Required if sell price is not null</errorMessage>
</ValidationRule>
